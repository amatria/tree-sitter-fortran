  "grammars": [
    {
      "name": "fortran",
      "camelcase": "Fortran",
      "title": "Fortran grammar for tree-sitter",
      "scope": "source.fortran",
      "file-types": [
        "f90"
      ],
      "injection-regex": "^fortran$",
      "class-name": "TreeSitterFortran"
    }
  ],
  "metadata": {
    "version": "0.0.0",
    "license": "MIT",
    "description": "Fortran grammar for tree-sitter",
    "authors": [
      {
        "name": "IÃ±aki Amatria Barral"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-fortran"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}